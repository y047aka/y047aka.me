{
  "scripts": {
    "clean": "rimraf docs",
    "echo:CNAME": "echo 'y047aka.me' > ./docs/CNAME",
    "echo": "mkdirp docs && npm-run-all -p echo:*",
    "watch:assets": "cpx -w \"src/assets/**/*\" dist/assets",
    "watch:parcel": "parcel src/index.html",
    "compile:assets": "cpx \"src/assets/**/*\" docs/assets",
    "compile:parcel": "parcel build src/index.html -d docs",
    "compile": "npm-run-all -p compile:*",
    "start": "npm-run-all -p watch:*",
    "build": "npm-run-all -s clean echo compile"
  },
  "devDependencies": {
    "bulma": "^0.8.0",
    "cpx2": "^2.0.0",
    "elm": "^0.19.1-3",
    "elm-analyse": "^0.16.5",
    "elm-format": "^0.8.2",
    "elm-live": "^4.0.1",
    "elm-test": "^0.19.1-revision2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.0",
    "sass": "^1.24.4"
  }
}
