{
  "name": "y047aka.github.io",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "copy:CNAME": "cp ./src/CNAME ./docs",
    "copy:nojekyll": "cp ./src/.nojekyll ./docs",
    "copy:html": "cp ./src/index.html ./src/404.html ./docs",
    "copy:md": "cp -r ./src/md ./docs",
    "copy": "npm-run-all -p copy:CNAME copy:nojekyll copy:html copy:md",
    "watch:js": "webpack --mode development ./src/js/index.js --output ./docs/js/main.js --watch --display minimal",
    "watch:sass": "node-sass --include-path sass ./src/sass/style.sass ./docs/css/style.css --watch --quiet",
    "watch:server": "browser-sync start --server ./docs --files \"**/*\"",
    "watch": "npm-run-all -p watch:*",
    "start": "npm-run-all -s copy watch"
  },
  "author": "y047aka",
  "license": "ISC",
  "dependencies": {
    "@hyperapp/html": "^1.1.1",
    "@hyperapp/router": "^0.7.1",
    "axios": "^0.18.0",
    "front-matter": "^3.0.1",
    "hyperapp": "^1.2.9",
    "marked": "^0.6.0"
  },
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0"
  }
}
